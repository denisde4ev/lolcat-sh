#!/ hint/sh

( set -eu;
	#cd ../d/hsb/
	cd -- "${0%/*}" || exit
	node -p '
	(function (){
		S="";
		c=require("colorcolor");
		for(var i=0; i<=360; i+=3) {
			S += require("colorcolor")( "hsv("+i+",80%,70%)" , "rgb" )
		};
		return S.replace(
			/rgb\(([^)]+),([^)]+),([^)]+)\)/gm,
			"\33[38;2;$1;$2;$3m\u{2588}"
			// â–ˆ -> \u{2588}
		)
	})()
	'
)
